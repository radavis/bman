<table>
  <% @unix_commands.each do |unix_command| %>
    <tr>
      <td>
        <a href="/unix_commands/<%= unix_command.name %>"><%= unix_command.name.truncate(25) %></a>(<%= unix_command.type %>)
      </td>
      <td><%= unix_command.description.truncate(75) %></td>
    </tr>
  <% end %>
</table>

<div class="footer">
  <a href="<%= @prev_path %>">&lt;&lt;PREV</a> |
  <a href="<%= @next_path %>">NEXT&gt;&gt;</a>
</div>
